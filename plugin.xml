<plugin name='transmart-workspace' version='1.2.1' grailsVersion='1.3.7 &gt; *'>
  <author>Himanso Sahni, David Newton</author>
  <title>Workspace plugin for tranSMART</title>
  <description>\
Adds workspace functionality to the tranSMART Dataset Explorer
</description>
  <documentation>http://grails.org/plugin/transmart-workspace</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>com.recomdata.grails.plugin.workspace.SaveAnalysisController</resource>
    <resource>com.recomdata.grails.plugin.workspace.SubsetController</resource>
    <resource>com.recomdata.grails.plugin.workspace.WorkspaceController</resource>
    <resource>com.recomdata.grails.plugin.workspace.SubsetService</resource>
  </resources>
  <dependencies />
  <behavior />
</plugin>